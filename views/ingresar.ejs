<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <title>Ejemplo</title>
  </head>
  <body>
    <h1>Video juegos ingresar</h1>

<div class="container-fluid">

  <a href="/"><button type="button" class="btn btn-warning">Cancelar</button></a>
    <form action="/ingresar_juego" method="POST">
      <div class="mb-3">
        
        <label for="id" class="form-label">Nombre del id</label>
        <input type="text" class="form-control" id="id" aria-describedby="emailHelp" name="id">
        
      </div>
      
      <div class="mb-3">
        
        <label for="nombre" class="form-label">Nombre del Video Juego</label>
        <input type="text" class="form-control" id="nombre" aria-describedby="emailHelp" name="nombre">
        
      </div>
      <div class="mb-3">
        <label for="puntos" class="form-label">Puntos maximos</label>
        <input type="text" class="form-control" id="puntos" aria-describedby="emailHelp" name="puntos">
        
      </div>
      <div class="mb-3">
        <label for="niveles" class="form-label">Niveles</label>
        <input type="text" class="form-control" id="niveles" aria-describedby="emailHelp" name="niveles">
        
      </div>
      <div class="mb-3">
        <label for="monto" class="form-label">Monto en ventas</label>
        <input type="text" class="form-control" id="monto" aria-describedby="emailHelp" name="monto">
        
      </div>
      <button type="submit" class="btn btn-primary">Agregar</button>
    </form>
<hr>
</div>

    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
